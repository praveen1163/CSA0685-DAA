#include<stdio.h>
#include<conio.h>
int main(){
	int n1,n2,i,gcd;
	printf("enter any two numbers:\n");
	scanf("%d%d",&n1,&n2);
	for(i=1;i<=n1&&i<=n2;i++)
	{
		if(n1%i==0&&n2%i==0)
		gcd=i;
	}
	printf("gcd of two numbers %d and %d id %d",n1,n2,gcd);
	return 0;
}
output:
enter any two numbers:
45
76
gcd of two numbers 45 and 76 id 1
